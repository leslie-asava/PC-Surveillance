{
    "name": "ICU Ad Blocker",
    "description": "remove ads from websites \n ازالة الإعلانات من المواقع",
    "manifest_version": 2,
    "version": "1.00",
    "background": {
        "scripts": ["ads_sites.js", "ICUAdsBlock.js"]
    },
    "icons": {
    "16": "icons/icu 16x16.png",
    "48": "icons/icu 48x48.png",
    "128": "icons/icu 128x128.png"
},
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}